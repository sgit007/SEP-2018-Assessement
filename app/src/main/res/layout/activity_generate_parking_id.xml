<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activity.BookingActivities.GenerateParkingIDActivity"
    tools:layout_editor_absoluteY="81dp">


    <TextView
        android:id="@+id/parkingIDtoUser"
        android:layout_width="146dp"
        android:layout_height="0dp"
        android:layout_marginBottom="70dp"
        android:layout_marginTop="142dp"
        android:text="ParkingIDtoUser"
        app:layout_constraintBottom_toTopOf="@+id/confirmBookingButtoninGeneratePage"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/backToMainButtoninGeneratePage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="110dp"
        android:layout_marginLeft="28dp"
        android:layout_marginStart="28dp"
        android:text="Cancel"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/confirmBookingButtoninGeneratePage"
        app:layout_constraintTop_toBottomOf="@+id/confirmBookingButtoninGeneratePage" />

    <Button
        android:id="@+id/confirmBookingButtoninGeneratePage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="74dp"
        android:gravity="center_vertical|center_horizontal"
        android:text="Confirm Booking"
        app:layout_constraintBottom_toTopOf="@+id/backToMainButtoninGeneratePage"
        app:layout_constraintStart_toStartOf="@+id/parkingIDtoUser"
        app:layout_constraintTop_toBottomOf="@+id/parkingIDtoUser" />

</android.support.constraint.ConstraintLayout>